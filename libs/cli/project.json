{
  "name": "cli",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/cli/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/cli",
        "main": "libs/cli/src/main.ts",
        "tsConfig": "libs/cli/tsconfig.lib.json"
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": ["coverage/libs/cli"],
      "options": {
        "config": "libs/cli/vitest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["libs/cli/**/*.ts"]
      }
    }
  },
  "tags": ["type:tool", "scope:shared"]
}
